<odoo>

    <record id="end_internal_phonecall_wizard" model="ir.ui.view">
        <field name="name">End Internal Phonecall Wizard</field>
        <field name="model">end_internal_phonecall.wizard</field>
        <field name="arch" type="xml">
            <form string="End Internal Phone Call" create="false" edit="false">
                <group>
                    <field name="name"/>
                </group>
                <footer>
                    <button string="Save" type="object" name="end_internal_phonecall" class="oe_highlight"/>
                    <button string="Cancel" special="cancel" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
