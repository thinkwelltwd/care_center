<odoo>

    <record id="view_config_settings_form_alias_name_sheet" model="ir.ui.view">
        <field name="name">res.config.settings.form.alias_name_prefix</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="project.view_config_settings"/>
        <field name="arch" type="xml">

            <xpath expr="//group[@name='timesheets']" position="before">
                <group string="Email Settings" col="2">
                    <field name="alias_name_prefix"/>
                </group>
            </xpath>

        </field>
    </record>

</odoo>
